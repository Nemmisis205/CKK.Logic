}
@model ShopModel
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<ul>
    @foreach (CKK.Logic.Models.Product item in Model.UOW.Products.GetAll())
    {
        <li>
            <a>@item.Name</a>
        </li>
    }
</ul>

